{
     "name": "rana-tuhin/laravel-voice-ai",
     "description": "AI Voice / Speech Processing package for Laravel",
     "type": "library",
     "license": "MIT",
     "authors": [
          {
               "name": "Tuhin Rana Mondal",
               "email": "aimlesstuhin@gmail.com"
          }
     ],
     "autoload": {
          "psr-4": {
               "RanaTuhin\\VoiceAI\\": "src/"
          }
     },
     "extra": {
          "laravel": {
               "providers": [
                    "RanaTuhin\\VoiceAI\\VoiceAIServiceProvider"
               ],
               "aliases": {
                    "VoiceAI": "RanaTuhin\\VoiceAI\\Facades\\VoiceAI"
               }
          }
     },
     "require": {
          "php": "^8.1",
          "illuminate/support": "^10.0|^11.0|^12.0"
     },
     "minimum-stability": "dev",
     "prefer-stable": true
}